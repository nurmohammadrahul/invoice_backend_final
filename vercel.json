{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "env": {
      "MONGOODB_URI": "@mongoodb_uri",
      "JWT_SECRET": "@jwt_secret",
      "ADMIN_USERNAME": "@admin_username",
      "ADMIN_PASSWORD": "@admin_password"
    }
  }